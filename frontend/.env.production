# Production Environment Variables для React приложения

# Backend API URL (заменить на реальный Render URL после деплоя)
VITE_API_URL=https://your-backend-app.onrender.com/api

# WebSocket URL
VITE_WS_URL=wss://your-backend-app.onrender.com

# App Configuration
VITE_APP_NAME=AgentFlow Video Editor
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_COLLABORATION=true
VITE_ENABLE_REAL_TIME=true
VITE_ENABLE_ANALYTICS=true

# Upload Limits
VITE_MAX_FILE_SIZE=500000000
VITE_SUPPORTED_FORMATS=mp4,mov,avi,webm

